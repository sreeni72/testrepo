pipeline {
    agent {
        label '!windows'
    }

      stages {
        stage('Build') {
          steps {
              script {                 
                    def branch_nem = env.GIT_BRANCH
                    if (branch_nem.contains("*/")) {
                        branch_nem = branch_nem.split("\\*/")[1]
                    }
               }
                echo 'Pulling... ' + branch_nem               
            }
        }
    }
}
